{
  "name": "@cieloazul310/jclub-financial-utils",
  "version": "0.0.1-alpha.3",
  "license": "MIT",
  "homepage": "https://github.com/cieloazul310/jclub-financial-table",
  "author": {
    "name": "cieloazul310",
    "url": "https://cieloazul310.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cieloazul310/jclub-financial-table.git",
    "directory": "packages/utils"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "tsx ./scripts/build.ts",
    "build": "tsdown",
    "dev": "npm run build -- --watch"
  },
  "devDependencies": {
    "@repo/eslint-config": "^0.0.1-alpha.3",
    "@repo/typescript-config": "^0.0.1-alpha.3",
    "@types/node": "^24.9.1",
    "eslint": "^9.34.0",
    "tsdown": "^0.18.2",
    "tsx": "^4.20.6",
    "yaml": "^2.8.1"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}