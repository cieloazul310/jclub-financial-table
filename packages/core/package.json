{
  "name": "@cieloazul310/jclub-financial",
  "version": "0.0.1-alpha.7",
  "license": "MIT",
  "homepage": "https://github.com/cieloazul310/jclub-financial-table",
  "author": {
    "name": "cieloazul310",
    "url": "https://cieloazul310.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cieloazul310/jclub-financial-table.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./data": {
      "import": {
        "types": "./dist/data.d.mts",
        "default": "./dist/data.mjs"
      },
      "require": {
        "types": "./dist/data.d.cts",
        "default": "./dist/data.cjs"
      }
    },
    "./data/*": {
      "import": {
        "types": "./dist/data/*.d.mts",
        "default": "./dist/data/*.mjs"
      },
      "require": {
        "types": "./dist/data/*.d.cts",
        "default": "./dist/data/*.cjs"
      }
    },
    "./statistics": {
      "import": {
        "types": "./dist/statistics.d.mts",
        "default": "./dist/statistics.mjs"
      },
      "require": {
        "types": "./dist/statistics.d.cts",
        "default": "./dist/statistics.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.mts",
        "default": "./dist/utils.mjs"
      },
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/types.mjs"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "npm run copy-data",
    "build": "tsdown",
    "copy-data": "tsx ./scripts/copy-data.ts"
  },
  "dependencies": {
    "@cieloazul310/jclub-financial-data": "^0.0.1-alpha.7",
    "@cieloazul310/jclub-financial-statistics": "^0.0.1-alpha.7",
    "@cieloazul310/jclub-financial-utils": "^0.0.1-alpha.7"
  },
  "devDependencies": {
    "@repo/eslint-config": "^0.0.1-alpha.7",
    "@repo/typescript-config": "^0.0.1-alpha.7",
    "eslint": "^9.39.2",
    "tsdown": "^0.18.2",
    "tsx": "^4.21.0"
  }
}