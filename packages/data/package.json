{
  "name": "@cieloazul310/jclub-financial-data",
  "version": "0.0.0",
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.mjs"
      },
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.mjs"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:template && npm run build:data && npm run build:types",
    "build:template": "npm run build:template:mjs && npm run build:template:cjs",
    "build:template:mjs": "tsc src/*.ts --module esnext --target esnext --skipLibCheck --moduleResolution bundler --declaration --outDir scripts/templates/mjs",
    "build:template:cjs": "tsc src/*.ts --module nodenext --target es5 --skipLibCheck --moduleResolution nodenext --outDir scripts/templates/cjs",
    "build:data": "tsx ./scripts/yaml-to-json.ts",
    "build:types": "tsx ./scripts/build-types.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@cieloazul310/jclub-financial-utils": "^0.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "^0.0.0",
    "@types/node": "^24.9.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  }
}