{
  "name": "@cieloazul310/jclub-financial-data",
  "version": "0.0.1-alpha.3",
  "license": "MIT",
  "homepage": "https://github.com/cieloazul310/jclub-financial-table",
  "author": {
    "name": "cieloazul310",
    "url": "https://cieloazul310.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cieloazul310/jclub-financial-table.git",
    "directory": "packages/data"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.mjs"
      },
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/types.mjs"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:template && npm run build:data && npm run build:types",
    "build:template": "npm run build:template:top && npm run build:template:clubs",
    "build:template:top": "tsdown",
    "build:template:clubs": "tsdown --config tsdown-for-clubs.config.ts",
    "build:data": "tsx ./scripts/yaml-to-json.ts",
    "build:types": "tsx ./scripts/build-types.ts"
  },
  "dependencies": {
    "@cieloazul310/jclub-financial-utils": "^0.0.1-alpha.3"
  },
  "devDependencies": {
    "@repo/eslint-config": "^0.0.1-alpha.3",
    "@repo/typescript-config": "^0.0.1-alpha.3",
    "@types/node": "^24.9.1",
    "eslint": "^9.34.0",
    "tsdown": "^0.18.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}