{
  "name": "jclub-financial-table",
  "private": true,
  "description": "Jリーグが毎年公開している「Jクラブ個別経営情報開示資料」の15年分のデータをクラブ別、年度別に表示したページ。",
  "version": "1.0.0",
  "author": "cieloazul310",
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/cieloazul310/jclub-financial-table"
  },
  "bugs": {
    "url": "https://github.com/cieloazul310/jclub-financial-table/issues"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -o -H 0.0.0.0 --prefix-paths",
    "deploy": "gh-pages -d public",
    "clean": "gatsby clean",
    "lint": "eslint src/ --fix --ignore-pattern \"**/stories/**/*\" && eslint gatsby/ --fix",
    "format": "prettier \"{src,gatsby}/**/*.{js,jsx,ts,tsx}\" --parser typescript --write",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:gatsby-node": "tsc gatsby-node/*.ts --module commonjs --target esnext --outDir gatsby-node-dist --skipLibCheck",
    "convert": "ts-node ./converter/index.ts",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cieloazul310/gatsby-theme-aoi": "^3.1.1",
    "@cieloazul310/gatsby-theme-aoi-blog-components": "^3.1.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@types/d3": "^7.4.0",
    "d3": "^7.8.2",
    "gatsby": "^5.7.0",
    "gatsby-plugin-eslint": "^4.0.3",
    "gatsby-plugin-google-gtag": "5.7.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-mdx": "^5.7.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-plugin-twitter": "5.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-transformer-yaml": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "remark-gfm": "^1.0.0",
    "swiper": "^9.1.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "csv-parse": "^5.3.6",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern \"**/stories/**/*\"",
      "prettier --parser typescript --write"
    ],
    "./gatsby/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}
