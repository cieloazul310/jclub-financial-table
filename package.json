{
  "name": "jclub-financial-table",
  "private": true,
  "description": "Jリーグが毎年公開している「Jクラブ個別経営情報開示資料」の15年分のデータをクラブ別、年度別に表示したページ。",
  "version": "1.0.0",
  "author": "cieloazul310",
  "dependencies": {
    "@cieloazul310/gatsby-theme-aoi": "^2.4.2",
    "@cieloazul310/gatsby-theme-aoi-blog-components": "^2.4.2",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@types/d3": "^7.1.0",
    "d3": "^7.4.0",
    "gatsby": "^4.11.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-google-analytics": "^4.11.0",
    "gatsby-plugin-manifest": "^4.11.1",
    "gatsby-plugin-mdx": "^3.11.1",
    "gatsby-plugin-sitemap": "^5.11.1",
    "gatsby-source-filesystem": "^4.11.1",
    "gatsby-transformer-yaml": "^4.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/d3-dsv": "^3.0.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet-async": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -o -H 0.0.0.0 --prefix-paths",
    "deploy": "gh-pages -d public",
    "clean": "gatsby clean",
    "lint": "eslint src/ --fix --ignore-pattern \"**/stories/**/*\" && eslint gatsby-node/ --fix",
    "format": "prettier \"{src,gatsby-node}/**/*.{js,jsx,ts,tsx}\" --parser typescript --write",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:gatsby-node": "tsc gatsby-node/*.ts --module commonjs --target esnext --outDir gatsby-node-dist --skipLibCheck",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cieloazul310/jclub-financial-table"
  },
  "bugs": {
    "url": "https://github.com/cieloazul310/jclub-financial-table/issues"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern \"**/stories/**/*\"",
      "prettier --parser typescript --write"
    ],
    "./src/gatsby-node/**/*": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}
