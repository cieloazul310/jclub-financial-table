{
  "name": "jclub-financial-table",
  "private": true,
  "description": "Jリーグが毎年公開している「Jクラブ個別経営情報開示資料」の15年分のデータをクラブ別、年度別に表示したページ。",
  "version": "1.0.0",
  "author": "cieloazul310",
  "dependencies": {
    "@cieloazul310/gatsby-theme-aoi": "^2.9.1",
    "@cieloazul310/gatsby-theme-aoi-blog-components": "^2.9.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@types/d3": "^7.4.0",
    "d3": "^7.6.1",
    "gatsby": "^4.24.6",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-google-analytics": "^4.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-mdx": "^3.19.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-plugin-twitter": "4.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-yaml": "^4.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.0",
    "swiper": "^8.4.5",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "csv-parse": "^5.3.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -o -H 0.0.0.0 --prefix-paths",
    "deploy": "gh-pages -d public",
    "clean": "gatsby clean",
    "lint": "eslint src/ --fix --ignore-pattern \"**/stories/**/*\" && eslint gatsby/ --fix",
    "format": "prettier \"{src,gatsby}/**/*.{js,jsx,ts,tsx}\" --parser typescript --write",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:gatsby-node": "tsc gatsby-node/*.ts --module commonjs --target esnext --outDir gatsby-node-dist --skipLibCheck",
    "convert": "ts-node ./converter/index.ts",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cieloazul310/jclub-financial-table"
  },
  "bugs": {
    "url": "https://github.com/cieloazul310/jclub-financial-table/issues"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern \"**/stories/**/*\"",
      "prettier --parser typescript --write"
    ],
    "./gatsby/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}
